{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Android JS",
      "type": "reactnative",
      "request": "launch",
      "platform": "android",
      "cwd": "${workspaceFolder}",
      "enableDebug": true
    },
    {
      "name": "Attach to Packager",
      "type": "reactnative",
      "request": "attach",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Run Android (No Debug)",
      "type": "reactnative",
      "request": "launch",
      "platform": "android",
      "cwd": "${workspaceFolder}",
      "enableDebug": false
    },
    {
      "name": "Dart: Run all Tests",
      "type": "dart",
      "request": "launch",
      "program": "./test/"
    }
  ]
}
